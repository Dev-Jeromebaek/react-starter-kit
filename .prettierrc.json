{
  "singleQuote": true,
  "jsxSingleQuote": true,
  "trailingComma": "all",
  "printWidth": 100,
  "arrowParens": "avoid",
  "tabWidth": 2,
  "rules": {
    "sort-imports": 2
  },
  "proseWrap": "preserve",
  "react/jsx-wrap-multilines": [
    "error",
    {
      "declaration": false,
      "assignment": false
    }
  ]
}
